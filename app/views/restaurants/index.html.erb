<h1>Restaurants</h1>

<div id="restaurants">
  <% @restaurants.each do |restaurant| %>
    <li><%= restaurant.name %></li>
    <p>
      <%= link_to "See details", restaurant_path(restaurant), class: "btn btn-primary"%>
    </p>
  <% end %>
</div>

<%= link_to "New restaurant", new_restaurant_path %>
